{
  "include": ["msgpackable.ts"],
  "compilerOptions": {
    "outDir": "./boilerplate",
    "inlineSourceMap": false,
    "inlineSources": false,
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "allowJs": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "declaration": false,
    "strict": true,
    "lib": ["esnext","esnext.string"]
  }
}
