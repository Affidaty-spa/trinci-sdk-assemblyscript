{
  "include": ["transformer.ts"],
  "exclude": [],
  "compilerOptions": {
    "outDir": "./",
    "inlineSourceMap": false,
    "inlineSources": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "allowJs": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "declaration": false,
    "strict": true,
    "lib": ["esnext","esnext.string"]
  }
}
