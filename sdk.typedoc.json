{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./sdk/sdk.ts"],
    "tsconfig": "./sdk/tsconfig.json",
    "out": "docs/sdk",
    "emit": "docs",
    "name": "TRINCI AssemblyScript SDK Documentation",
    "theme": "default",
    "darkHighlightTheme": "dark-plus",
    "excludePrivate": true,
    "externalPattern": [
        "**/node_modules/**",
        "node_modules/**"
    ],
    "excludeExternals": true
}