{
    "name": "as_smart_contract",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "asbuild:debug": "asc assembly/index.ts --config asconfig.json --target debug --transform ./msgpackable --use abort=",
        "asbuild:release": "asc assembly/index.ts --config asconfig.json --target release --transform ./msgpackable --use abort=",
        "asbuild": "npm run asbuild:release",
        "asbuild:docker": "cd ./utils/docker && make smartcontract-build",
        "test": "jest --no-coverage",
        "docker:init": "cd ./utils/docker && make containers-init",
        "node:start": "cd ./utils/docker && make node-start",
        "node:stop": "cd ./utils/docker && make node-stop",
        "node:reset": "cd ./utils/docker && make node-reset",
        "node:logs": "cd ./utils/docker && make node-logs",
        "publish": "./utils/publish.js",
        "publish:local": "npm run publish -- -u \"http://localhost\"",
        "publish:zip": "npm run publish -- -z \"./preapprove.trinci\"",
        "publish:mainnet": "npm run publish -- -s",
        "publish:help": "npm run publish -- --help",
        "relay": "./utils/socketRelay.js"
    },
    "author": "",
    "license": "",
    "dependencies": {
        "@affidaty/t2-lib": "^2.0.15",
        "@affidaty/trinci-sdk-as": "^2.3.5",
        "yargs": "^17.4.1",
        "adm-zip": "^0.5.9"
    },
    "devDependencies": {
        "@assemblyscript/loader": "0.19.23",
        "assemblyscript": "0.19.23"
    }
}
