{
    "name": "trinci_smart_contract",
    "version": "0.0.1",
    "description": "A smart contract made for TRINCI blockchain.",
    "scripts": {
        "asbuild:debug": "asc assembly/index.ts --config asconfig.json --target debug --transform ./msgpackable.mjs --use abort=",
        "asbuild:release": "asc assembly/index.ts --config asconfig.json --target release --transform ./msgpackable.mjs --use abort=",
        "asbuild": "npm run asbuild:release",
        "asbuild:docker": "cd ./utils/docker && make smartcontract-build",
        "test": "jest --no-coverage",
        "docker:init": "cd ./utils/docker && make containers-init",
        "node:start": "cd ./utils/docker && make node-start",
        "node:stop": "cd ./utils/docker && make node-stop",
        "node:reset": "cd ./utils/docker && make node-reset",
        "node:logs": "cd ./utils/docker && make node-logs",
        "publish": "./utils/publish.js",
        "publish:local": "npm run publish -- -n \"QmNiibPaxdU61jSUK35dRwVQYjF9AC3GScWTRzRdFtZ4vZ\" -u \"http://localhost:8000\"",
        "publish:zip": "npm run publish -- -z \"./preapprove.trinci\"",
        "publish:mainnet": "npm run publish -- -s",
        "publish:testnet": "npm run publish -- -n \"QmcvHfPC6XYpgxvJSZQCVBd7QAMEHnLbbK1ytA4McWx5UY\" -u \"https://testnet.trinci.net\"",
        "publish:help": "npm run publish -- --help",
        "relay": "./utils/socketRelay.js"
    },
    "author": "",
    "license": "",
    "dependencies": {
        "@affidaty/t2-lib": "^2.1.0",
        "@affidaty/trinci-sdk-as": "^2.4.5",
        "@types/yargs": "^17.0.13",
        "yargs": "^17.6.2",
        "adm-zip": "^0.5.9"
    },
    "devDependencies": {
        "@assemblyscript/loader": "0.22.0",
        "assemblyscript": "0.22.0"
    }
}
