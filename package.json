{
    "name": "@affidaty/trinci-sdk-as",
    "version": "3.0.0-beta2",
    "description": "Trinci SDK for Assemblyscript smart contract development.",
    "type": "module",
    "exports": {
        ".": {
            "import": "./index.ts"
        },
        "./sdk": {
            "import": "./sdk/sdk.ts"
        },
        "./visitor-as": {
            "import": "./visitor-as/index.js",
            "types": "./visitor-as/index.d.ts"
        },
        "./*": "./*"
    },
    "files": [
        "boilerplate",
        "sdk",
        "visitor-as",
        "ts_jest",
        "index.ts",
        "init.sh",
        "LICENSE",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build:visitor": "tsc -p tsconfig.visitor.json",
        "build:transformer": "tsc -p tsconfig.transformer.json && mv ./boilerplate/msgpackable.js ./boilerplate/msgpackable.mjs",
        "build:tsjest-esm": "tsc -p tsconfig.tsjest.esm.json",
        "build:tsjest-cjs": "tsc -p tsconfig.tsjest.cjs.json",
        "build:tsjest": "npm run build:tsjest-esm && npm run build:tsjest-cjs",
        "build": "npm run build:visitor && npm run build:transformer",
        "docs:sdk": "typedoc --options sdk.typedoc.json --skipErrorChecking",
        "docs:testenv": "typedoc --options testenv.typedoc.json --skipErrorChecking",
        "docs": "npm run docs:sdk && npm run docs:testenv",
        "pre-publish": "npm run build && npm run docs"
    },
    "author": "Affidaty S.p.A",
    "license": "AGPL-3.0-only",
    "keywords": [
        "blockchain",
        "affidaty",
        "trinci",
        "assemblyscript",
        "smart contract"
    ],
    "dependencies": {
        "@affidaty/t2-lib": "^2.3.0",
        "@affidaty/trinci-sdk-as": "^3.0.0-beta2",
        "@msgpack/msgpack": "^3.0.0-beta2",
        "@types/cli-table": "^0.3.1",
        "@types/jest": "^29.5.4",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/msgpack-lite": "^0.1.8",
        "@wapc/as-msgpack": "^0.1.15",
        "cli-table": "^0.3.11",
        "fast-sha256": "^1.3.0",
        "jest": "^29.6.4",
        "lodash.clonedeep": "^4.5.0",
        "msgpack-lite": "^0.1.26",
        "ts-jest": "^29.0.3",
        "ts-mixer": "^6.0.2",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@assemblyscript/loader": "0.27.9",
        "assemblyscript": "0.27.9",
        "typedoc": "^0.25.0",
        "typedoc-plugin-missing-exports": "^2.1.0",
        "typescript": "^5.2.2"
    }
}
