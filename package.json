{
    "name": "@affidaty/trinci-sdk-as",
    "version": "2.3.23",
    "description": "Trinci SDK for Assemblyscript smart contract development.",
    "main": "index.ts",
    "files": [
        "boilerplate",
        "sdk",
        "jestenv",
        "visitor-as-cjs",
        "index.ts",
        "init.sh",
        "LICENSE",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build:visitor": "tsc -p tsconfig.visitor.json && cp -r visitor-as/as visitor-as-cjs/as",
        "build:jestenv-esm": "tsc -p tsconfig.jestenv.esm.json",
        "build:jestenv-cjs": "tsc -p tsconfig.jestenv.cjs.json",
        "build:jestenv": "npm run build:jestenv-esm && npm run build:jestenv-cjs",
        "build": "npm run build:visitor && npm run build:jestenv"
    },
    "author": "Affidaty S.p.A",
    "license": "AGPL-3.0-only",
    "keywords": [
        "blockchain",
        "affidaty",
        "trinci",
        "assemblyscript",
        "smart contract"
    ],
    "dependencies": {
        "@types/cli-table": "^0.3.0",
        "@types/jest": "^27.4.1",
        "@types/msgpack-lite": "^0.1.8",
        "@wapc/as-msgpack": "^0.1.13",
        "cli-table": "^0.3.11",
        "fast-sha256": "^1.3.0",
        "jest": "^27.5.1",
        "lodash.clonedeep": "^4.5.0",
        "msgpack-lite": "^0.1.26",
        "ts-jest": "^27.1.3",
        "ts-mixer": "^6.0.0",
        "ts-node": "^10.2.1"
    },
    "devDependencies": {
        "@assemblyscript/loader": "^0.19.11",
