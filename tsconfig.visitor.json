{
  "compilerOptions": {
    "outDir": "./visitor-as-cjs",
    "inlineSourceMap": true,
    "inlineSources": true,
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "allowJs": false,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "declaration": true,
    "strict": true,
    "lib": ["esnext","esnext.string"]
  },
  "include": ["./visitor-as/*"]
}
